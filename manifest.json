{
  "name": "nicoauth",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "自動枠移動できたらいいな",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons" : {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://live.nicovideo.jp/*", "http://com.nicovideo.jp/*"],
      "js": ["j.js", "account.js"],
      "css": ["css.css"],
      "run_at": "document_idle"
    }
  ]
}
